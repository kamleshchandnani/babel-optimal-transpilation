{
  "env": {
    "plain": {
      "presets": ["@babel/preset-env", "@babel/preset-react"]
    },
    "spread": {
      "presets": ["@babel/preset-env", "@babel/preset-react", "@babel/preset-stage-2"]
    },
    "cust": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "targets": {
              "browsers": ["last 2 Chrome versions", "safari >=11"]
            },
            "modules": false
          }
        ],
        "@babel/preset-react",
        "@babel/preset-stage-2"
      ]
    }
  }
}
